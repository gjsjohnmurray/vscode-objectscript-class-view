{
  "name": "objectscript-class-view",
  "displayName": "InterSystems ObjectScript Class View",
  "description": "View and navigate all class members, including inherited ones",
  "publisher": "georgejames",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
		"intersystems",
		"objectscript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gjsjohnmurray/vscode-objectscript-class-view"
  },
  "extensionDependencies": [
    "intersystems-community.vscode-objectscript",
    "intersystems-community.servermanager",
    "intersystems.language-server"
  ],
  "activationEvents": [
    "onLanguage:objectscript-class"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "axios": "^1.2.4",
		"vscode-cache": "^0.3.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "ts-loader": "^9.4.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@vscode/test-electron": "^2.1.5"
  }
}
